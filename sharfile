 <TblColumn 
  <TblColumnNum 0>
  <TblColumnWidth  1.0">
  <TblColumnH 
   <Pgf 
    <PgfTag `CellHeading'>
    <PgfAlignment Center >
    <PgfFont 
     <FTag `'>
     <FFamily `Times'>
     <FVar `Regular'>
     <FWeight `Regular'>
     <FAngle `Regular'>
     <FPostScriptName `Times-Roman'>
     <FSize  12.0 pt>
     <FUnderlining FNoUnderlining >
     <FUnderline No >
     <FDoubleUnderline No >
     <FNumericUnderline No >
     <FOverline No >
     <FStrike No >
     <FChangeBar No >
     <FOutline No >
     <FShadow No >
     <FPairKern Yes >
     <FCase FAsTyped >
     <FPosition FNormal >
     <FSupScript No >
